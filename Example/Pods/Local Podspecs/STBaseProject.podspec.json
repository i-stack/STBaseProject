{
  "name": "STBaseProject",
  "version": "0.1.0",
  "summary": "Collect common classes in the development process.",
  "description": "Collect common classes in the development process. Can custom.",
  "homepage": "https://github.com/songMW/STBaseProject",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "songMW": "songshoubing7664@163.com"
  },
  "source": {
    "git": "https://github.com/songMW/STBaseProject.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "resources": [
    "STBaseProject/Assets/*"
  ],
  "default_subspecs": "STBase",
  "public_header_files": "Pod/Classes/**/*",
  "subspecs": [
    {
      "name": "STBase",
      "subspecs": [
        {
          "name": "STBaseViewController",
          "source_files": [
            "STBaseProject/Classes/STBase/STBaseViewController/*.swift"
          ]
        },
        {
          "name": "STBaseView",
          "source_files": [
            "STBaseProject/Classes/STBase/STBaseView/*.swift"
          ]
        },
        {
          "name": "STBaseViewMode",
          "source_files": [
            "STBaseProject/Classes/STBase/STBaseViewMode/*.swift"
          ]
        },
        {
          "name": "STBaseModel",
          "source_files": [
            "STBaseProject/Classes/STBase/STBaseModel/*.swift"
          ]
        },
        {
          "name": "STBaseConstants",
          "source_files": [
            "STBaseProject/Classes/STBase/STBaseConstants/*.swift"
          ]
        }
      ]
    },
    {
      "name": "STCommon",
      "source_files": "STBaseProject/Classes/STCommon/*.swift"
    },
    {
      "name": "STExtension",
      "source_files": "STBaseProject/Classes/STExtension/*.swift"
    },
    {
      "name": "STUI",
      "subspecs": [
        {
          "name": "STBtn",
          "source_files": [
            "STBaseProject/Classes/STUI/STBtn/*.swift"
          ]
        },
        {
          "name": "STScanner",
          "source_files": [
            "STBaseProject/Classes/STUI/STScanner/*.swift"
          ]
        },
        {
          "name": "STContract",
          "source_files": [
            "STBaseProject/Classes/STUI/STContract/*.swift"
          ]
        },
        {
          "name": "STScreenshot",
          "source_files": [
            "STBaseProject/Classes/STUI/STScreenshot/*.swift"
          ]
        }
      ]
    }
  ]
}
