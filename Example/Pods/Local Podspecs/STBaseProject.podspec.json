{
  "name": "STBaseProject",
  "version": "2.0.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "A powerful iOS base project with modular architecture and rich UI components.",
  "description": "STBaseProject is a comprehensive iOS development framework that provides:\n- Modular STKit toolset with Core/UI/Network/Media/Scan/Security/Localization/Location modules\n- Complete MVVM architecture with STBaseViewController, STBaseViewModel, STBaseModel, STBaseView\n- Rich UI components including custom buttons, labels, alerts, and scan views\n- Unified image management, network session, localization, and security tools\n- Easy integration with CocoaPods, supports selective module importing",
  "homepage": "https://github.com/i-stack/STBaseProject",
  "authors": {
    "i-stack": "songshoubing7664@163.com"
  },
  "source": {
    "git": "https://github.com/i-stack/STBaseProject.git",
    "tag": "2.0.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2",
    "5.3",
    "5.4",
    "5.5",
    "5.6",
    "5.7",
    "5.8",
    "5.9"
  ],
  "requires_arc": true,
  "documentation_url": "https://github.com/i-stack/STBaseProject/blob/main/README.md",
  "readme": "https://github.com/i-stack/STBaseProject/blob/main/README.md",
  "default_subspecs": [
    "STBaseModule",
    "STConfig",
    "STKit/Core",
    "STKit/UI",
    "STKit/Network",
    "STKit/Security",
    "STKit/Localization"
  ],
  "subspecs": [
    {
      "name": "STBaseModule",
      "source_files": "STBaseProject/Classes/STBaseModule/**/*.swift"
    },
    {
      "name": "STConfig",
      "source_files": "STBaseProject/Classes/STConfig/**/*.swift"
    },
    {
      "name": "STKit",
      "subspecs": [
        {
          "name": "Core",
          "source_files": "STBaseProject/Classes/STKit/Core/**/*.swift",
          "dependencies": {
            "STBaseProject/STConfig": []
          }
        },
        {
          "name": "UI",
          "source_files": "STBaseProject/Classes/STKit/UI/**/*.swift",
          "dependencies": {
            "STBaseProject/STBaseModule": [],
            "STBaseProject/STKit/Core": []
          }
        },
        {
          "name": "Network",
          "source_files": "STBaseProject/Classes/STKit/Network/**/*.swift",
          "dependencies": {
            "STBaseProject/STBaseModule": [],
            "STBaseProject/STKit/Core": []
          }
        },
        {
          "name": "Security",
          "source_files": "STBaseProject/Classes/STKit/Security/**/*.swift",
          "dependencies": {
            "STBaseProject/STBaseModule": [],
            "STBaseProject/STKit/Core": []
          }
        },
        {
          "name": "Localization",
          "source_files": "STBaseProject/Classes/STKit/Localization/**/*.swift",
          "dependencies": {
            "STBaseProject/STBaseModule": [],
            "STBaseProject/STKit/Core": []
          }
        },
        {
          "name": "Location",
          "source_files": "STBaseProject/Classes/STKit/Location/**/*.swift",
          "dependencies": {
            "STBaseProject/STBaseModule": [],
            "STBaseProject/STKit/Core": []
          }
        },
        {
          "name": "Scan",
          "source_files": "STBaseProject/Classes/STKit/Scan/**/*.swift",
          "dependencies": {
            "STBaseProject/STBaseModule": [],
            "STBaseProject/STKit/Core": []
          }
        },
        {
          "name": "Media",
          "source_files": "STBaseProject/Classes/STKit/Media/**/*.swift",
          "dependencies": {
            "STBaseProject/STBaseModule": [],
            "STBaseProject/STKit/Core": []
          }
        }
      ]
    }
  ],
  "swift_version": "5.9"
}
