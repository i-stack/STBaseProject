{
  "name": "STBaseProject",
  "version": "0.1.0",
  "summary": "Collect common classes in the development process.",
  "description": "Collect common classes in the development process. Can custom.",
  "homepage": "https://github.com/songMW/STBaseProject",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "songMW": "songshoubing7664@163.com"
  },
  "source": {
    "git": "https://github.com/songMW/STBaseProject.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "STBaseProject/Classes/**/*",
  "default_subspecs": "STBaseViewController",
  "public_header_files": "Pod/Classes/**/*",
  "subspecs": [
    {
      "name": "STBaseViewController",
      "source_files": "STBaseProject/Classes/STBaseViewController/*.swift",
      "resource_bundles": {
        "STBaseProject": [
          "STBaseProject/Assets/default_subspec/*.png"
        ]
      }
    },
    {
      "name": "STContract",
      "source_files": "STBaseProject/Classes/STContract/*.swift"
    },
    {
      "name": "STScreenshot",
      "source_files": "STBaseProject/Classes/STScreenshot/*.swift"
    },
    {
      "name": "STScanner",
      "source_files": "STBaseProject/Classes/STScanner/*.swift",
      "resource_bundles": {
        "STBaseProject": [
          "STBaseProject/Assets/STScanner/*.png"
        ]
      }
    }
  ]
}
