{
  "name": "STBaseProject",
  "version": "1.1.4",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "A powerful iOS base project with modular architecture and rich UI components.",
  "description": "STBaseProject is a comprehensive iOS development framework that provides:\n- Modular STBaseModule with Core/UI/Security/Config modules\n- Complete MVVM architecture with STBaseViewController, STBaseViewModel, STBaseModel, STBaseView\n- Rich UI components including custom buttons, labels, alerts, and scan views\n- Unified image management, network session, localization, and security tools\n- Easy integration with CocoaPods and SPM, supports selective module importing",
  "homepage": "https://github.com/i-stack/STBaseProject",
  "authors": {
    "i-stack": "songshoubing7664@163.com"
  },
  "source": {
    "git": "https://github.com/i-stack/STBaseProject.git",
    "tag": "1.1.4"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": [
    "5.0",
    "5.1",
    "5.2",
    "5.3",
    "5.4",
    "5.5",
    "5.6",
    "5.7",
    "5.8",
    "5.9"
  ],
  "requires_arc": true,
  "documentation_url": "https://github.com/i-stack/STBaseProject/blob/main/README.md",
  "readme": "https://github.com/i-stack/STBaseProject/blob/main/README.md",
  "default_subspecs": [
    "STBaseProject"
  ],
  "subspecs": [
    {
      "name": "STBaseProject",
      "source_files": "Sources/STBaseProject/**/*.swift"
    },
    {
      "name": "STLocation",
      "source_files": "Sources/STLocation/**/*.swift",
      "dependencies": {
        "STBaseProject/STBaseProject": []
      }
    },
    {
      "name": "STScan",
      "source_files": "Sources/STScan/**/*.swift",
      "dependencies": {
        "STBaseProject/STBaseProject": []
      }
    },
    {
      "name": "STMedia",
      "source_files": "Sources/STMedia/**/*.swift",
      "dependencies": {
        "STBaseProject/STBaseProject": []
      },
      "pod_target_xcconfig": {
        "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Sources/STBaseProject"
      }
    },
    {
      "name": "STDialog",
      "source_files": "Sources/STDialog/**/*.swift",
      "dependencies": {
        "STBaseProject/STBaseProject": []
      },
      "pod_target_xcconfig": {
        "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Sources/STBaseProject"
      }
    }
  ],
  "swift_version": "5.9"
}
